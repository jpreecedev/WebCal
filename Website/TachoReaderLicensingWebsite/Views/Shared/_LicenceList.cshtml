@model IEnumerable<TachoReaderLicensingWebsite.DataModel.Licence>


@if (Model.Count() > 0)
{
    <ul id="licenceListRows">
        @foreach (var item in Model)
        { if (item.ExpiryDate < DateTime.Now)
      {
            <li class="licence expiredLicence" data-lid="@item.AccessID"><span class="span6">@item.LKey</span>
           
            <span class="span5">@item.ExpiryDate.ToString("dd/MM/yyyy")</span>

            <a href="#" class="span1" rel="delete"><i class="icon-remove icon-white"></i></a></li>
      }

      else
      {
               <li class="licence " data-lid="@item.AccessID"><span class="span6">@item.LKey</span>
           
            <span class="span5">@item.ExpiryDate.ToString("dd/MM/yyyy")</span>

            <a href="#" class="span1" rel="delete"><i class="icon-remove icon-white"></i></a></li>

      }
        }
    </ul>
}
else
{
    <p>This client currently has no licences</p>
}
